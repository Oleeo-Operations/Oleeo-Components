{
  "compilerOptions": {
    "jsx": "react",
    "module": "CommonJS",
    "noImplicitAny": true,
    "outDir": "./build/",
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "removeComments": true,
    "sourceMap": false,
    "target": "ES5"
  },
  "exclude": ["node_modules", "**/*.spec.tsx"],
  "include": ["**/*.ts", "**/*.tsx"]
}
